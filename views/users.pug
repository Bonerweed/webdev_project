block content
  h1 List of users
  table
    tr
      th #
      th ID
      th Username
      th Password
      th Remove
    each user, index in users
      tr
        td= index
        td= user["id"]
        td= user["username"]
        td= user["password"]
        td
          button(id= index onclick= removeUser)
  script(src= "/javascript/users.js")
